{
  "title": "Host Metrics (OTEL Collector)",
  "timezone": "browser",
  "schemaVersion": 37,
  "version": 1,
  "panels": [
    {
      "type": "timeseries",
      "title": "CPU Utilization %",
      "targets": [
        {
          "expr": "100 * (1 - avg by (cpu)(rate(system_cpu_time_seconds_total{state=\"idle\"}[1m])))",
          "legendFormat": "CPU {{cpu}}",
          "interval": ""
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 9,
        "w": 24,
        "x": 0,
        "y": 0
      }
    },
    {
      "type": "timeseries",
      "title": "Memory Usage %",
      "targets": [
        {
          "expr": "100 * (system_memory_used_bytes / system_memory_total_bytes)",
          "legendFormat": "Memory Usage",
          "interval": ""
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 9,
        "w": 24,
        "x": 0,
        "y": 9
      }
    }
  ]
}
